<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itheima.ftpfile.mapper.usermapper">
    <select id="selectuser" resultType="com.itheima.ftpfile.ftpclass.FTPuser">
        select * from ftpusers where user=#{user} and password=#{password}
    </select>
</mapper>